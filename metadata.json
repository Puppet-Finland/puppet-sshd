{
  "name": "puppetfinland-sshd",
  "version": "2.2.3",
  "author": "Puppet-Finland team",
  "summary": "Setup sshd with optional firewall and monit support",
  "license": "BSD-2-Clause",
  "source": "https://github.com/Puppet-Finland/puppet-sshd.git",
  "project_page": "https://github.com/Puppet-Finland/puppet-sshd",
  "dependencies": [
    {
      "name": "puppetfinland/monit",
      "version_requirement": ">= 0.5.0"
    },
    {
      "name": "puppetfinland/packetfilter",
      "version_requirement": ">= 0.5.0"
    },
    {
      "name": "puppetfinland/os",
      "version_requirement": ">= 0.5.5"
    },
    {
      "name": "puppetfinland/systemd",
      "version_requirement": ">= 0.1.0"
    },
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "7",
        "8",
        "9"
      ]
    },
    {
      "operatingsystem": "FreeBSD"
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "12.04",
        "14.04",
        "16.04"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 4.7.0 < 6.0.0"
    }
  ],
  "pdk-version": "1.4.1",
  "template-url": "file:///opt/puppetlabs/pdk/share/cache/pdk-templates.git",
  "template-ref": "1.4.1-0-g52adbbb"
}
